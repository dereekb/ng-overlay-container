{
  "name": "ng-overlay-container-demo",
  "version": "14.0.0-beta",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "yarn run copy-readme && ng build --configuration production",
    "test": "ng test ng-overlay-container",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-lib:prod": "ng build ng-overlay-container --configuration production",
    "copy-readme": "cpy README.md projects/ng-overlay-container && cpy README.md src/assets",
    "npm-publish": "npm publish ./dist/ng-overlay-container",
    "deploy:pipeline": "yarn run build-lib:prod && yarn publish ./dist/ng-overlay-container"
  },
  "module": "./dist/fesm2015/ng-overlay-container.mjs",
  "es2020": "./dist/fesm2020/ng-overlay-container.mjs",
  "esm2020": "./dist/esm2020/ng-overlay-container.mjs",
  "fesm2020": "./dist/fesm2020/ng-overlay-container.mjs",
  "fesm2015": "./dist/fesm2015/ng-overlay-container.mjs",
  "typings": "./dist/index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./dist/package.json"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "esm2020": "./dist/esm2020/ng-overlay-container.mjs",
      "es2020": "./dist/fesm2020/ng-overlay-container.mjs",
      "es2015": "./dist/fesm2015/ng-overlay-container.mjs",
      "node": "./dist/fesm2015/ng-overlay-container.mjs",
      "default": "./dist/fesm2020/ng-overlay-container.mjs"
    }
  },
  "peerDependencies": {
    "@angular/cdk": "^14.0.0",
    "@angular/common": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/material": "^14.0.0"
  }
}
